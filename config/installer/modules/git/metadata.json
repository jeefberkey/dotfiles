{
  "name": "Aethylred-git",
  "version": "0.3.1",
  "author": "Aaron Hicks",
  "summary": "A module for installing and managing Git",
  "license": "Licensed under GPL3",
  "source": "https://github.com/aethylred/puppet-git",
  "project_page": "https://github.com/aethylred/puppet-git",
  "issues_url": "https://github.com/aethylred/puppet-git/issues",
  "tags": [
    "git"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "13.10",
        "14.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "ArchLinux"
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">=3.2.0 <5.0.0"},
    {"name":"puppetlabs/vcsrepo","version_requirement":">=1.0.0 <2.0.0"}
  ]
}
