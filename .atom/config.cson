"*":
  "activate-power-mode":
    comboMode: {}
    excludedFileTypes:
      excluded: [
        "pp"
        "rb"
      ]
    particles:
      colours:
        type: "random"
  aligner:
    alignComments: true
  "atom-beautify":
    general:
      _analyticsUserId: "aad78b8c-25a5-4445-9b1b-b8f18a07589c"
  "autocomplete-plus":
    enableExtendedUnicodeSupport: true
  core:
    autoHideMenuBar: true
    closeDeletedFileTabs: true
    disabledPackages: [
      "background-tips"
      "autosave"
      "welcome"
      "hidpi"
      "language-ruby-on-rails"
      "language-gfm"
      "spell-check"
    ]
    excludeVcsIgnoredPaths: false
    projectHome: "/home/nick/simpdev"
    themes: [
      "one-dark-ui"
      "oceanic-next"
    ]
  editor:
    fontFamily: "Source Code Pro Medium"
    fontSize: 12
    invisibles: {}
    showIndentGuide: true
  "env-from-shell": {}
  "exception-reporting":
    userId: "276f8864-fdd1-5b53-357e-8944a1ef1475"
  "file-icons":
    onChanges: true
  "git-diff": {}
  hidpi:
    scaleFactor: 1.2
  linter: {}
  "linter-puppet-lint":
    oldVersion: false
    skip140Chars: true
    skipAutoloaderLayout: true
    skipClassInherithsFromParamClass: true
  "linter-ui-default": {}
  "markdown-preview": {}
  "nord-atom-ui":
    darkerFormFocusEffect: true
  "one-dark-ui":
    fontSize: 11
    layoutMode: "Compact"
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
    syncHorizontalScroll: true
  "tree-view":
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
".ldif.text":
  editor:
    preferredLineLength: 140
".puppet.source":
  editor:
    preferredLineLength: 140
